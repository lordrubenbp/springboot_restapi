<?xml version="1.0" encoding="UTF-8"?>
<persistence xmlns="http://java.sun.com/xml/ns/persistence" version="2.0">

    <persistence-unit name="persistenceUnit">
        <provider>org.hibernate.jpa.HibernatePersistenceProvider</provider>
        <class>es.rubenbp.springbootrestexample.entity.Modelo</class>
        <properties>
            <property name="hibernate.connection.url" value="jdbc:mysql://localhost:3306/modelos3d"/>
            <property name="hibernate.connection.driver_class" value="com.mysql.jdbc.Driver"/>
            <property name="hibernate.connection.username" value="ruben"/>
            <property name="hibernate.connection.password" value="ruben"/>
        </properties>
    </persistence-unit>
</persistence>
